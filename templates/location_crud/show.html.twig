{% extends 'base.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>Location</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>CityName</th>
                <td>{{ location.CityName }}</td>
            </tr>
            <tr>
                <th>CountryCode</th>
                <td>{{ location.CountryCode }}</td>
            </tr>
            <tr>
                <th>Lon</th>
                <td>{{ location.Lon }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ location.Lat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_location_crud_index') }}">back to list</a>

    <a href="{{ path('app_location_crud_edit', {'id': location.id}) }}">edit</a>

    {{ include('location_crud/_delete_form.html.twig') }}
{% endblock %}
